<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="theme/uikit/db" VERSION="20140127" COMMENT="XMLDB file for Moodle theme/uikit"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="theme_uikit_less_settings" COMMENT="Table for holding less variables and configuration for the visual style manager">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="setting" TYPE="char" LENGTH="200" NOTNULL="true" SEQUENCE="false" COMMENT="Variable (starting with @) or other settings such as the uikit base theme and custom less code."/>
        <FIELD NAME="value" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>